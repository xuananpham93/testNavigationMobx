{
    "extends": "rallycoding",
    "rules": {
        "react/require-extension": "off",
        "arrow-body-style": 0,
        "no-mixed-operators": 0,
        "global-require": 0,
        "camelcase": 0,
        "prefer-template": 0,
        "default-case": 0,
        "no-else-return": 0,
        "prefer-const": 0,
        "no-underscore-dangle": 0,
        "max-len": 0,
        "no-lonely-if": 0,
        "eqeqeq": 0,
        "object-shorthand": 0
    },
    "globals": {
        "fetch": false
    }
}